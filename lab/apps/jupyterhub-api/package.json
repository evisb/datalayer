{
  "name": "@datalayer/datalab-jupyterhub",
  "version": "0.1.1",
  "scripts": {
    "build": "tsc && webpack",
    "clean": "rimraf lib build",
    "clean-all": "rimraf lib build",
    "prepublishOnly": "npm run build",
    "watch": "concurrently --kill-others \"jupyterhub\" \"tsc -w\" \"webpack --watch\" \"webpack-dev-server\""
  },
