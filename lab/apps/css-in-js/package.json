{
  "name": "@datalayer/datalab-css-in-js",
  "description": "Sample User Interface in React.js with Typescript.",
  "version": "0.0.1",
  "scripts": {
    "build": "tsc && BUILD_MODE=none webpack",
    "build:prod": "tsc && BUILD_MODE=production webpack",
    "clean": "rimraf lib dist build",
    "now-build": "tsc && BUILD_MODE=production webpack",
    "prepublishOnly": "BUILD_MODE=none npm run build",
    "start": "webpack-dev-server",
    "watch": "concurrently --kill-others \"tsc -w\" \"BUILD_MODE=none webpack --watch\" \"webpack-dev-server\""
  },
  "license": "Apache-2.0",
  "author": "Datalayer",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "images/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "es6-promise": "~4.1.1",
    "prop-types": "15.6.1",
    "react": "16.6.3",
    "react-dom": "16.6.3"
  },
  "devDependencies": {
    "@types/prop-types": "15.5.2",
    "@types/react": "16.7.6",
    "@types/react-dom": "16.0.9",
    "concurrently": "3.5.1",
    "css-loader": "1.0.0",
    "file-loader": "1.1.11",
    "less": "3.0.4",
    "less-loader": "4.1.0",
    "uglifyjs-webpack-plugin": "1.2.7",
    "mini-css-extract-plugin": "0.4.1",
    "optimize-css-assets-webpack-plugin": "5.0.0",
    "raw-loader": "0.5.1",
    "rimraf": "~2.6.2",
    "style-loader": "0.21.0",
    "styled-components": "4.1.2",
    "styled-system": "3.1.11",
    "typescript": "~2.8.3",
    "url-loader": "1.0.1",
    "watch": "~1.0.2",
    "webpack": "4.8.1",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.4",
    "whatwg-fetch": "~2.0.3"
  },
  "resolutions": {
    "@types/**/@types/react": "16.7.6"
  }
}
